{
  "name": "arrowpay/arrowbaze",
  "description": "Package for Orange Money Integration by ArrowBaze",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "ArrowBaze",
      "email": "hello@arrowbaze.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Arrowpay\\ArrowBaze\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arrowpay\\ArrowBaze\\ArrowBazeServiceProvider"
      ],
      "aliases": {
        "ArrowPay": "Arrowpay\\ArrowBaze\\Facades\\ArrowPay"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^12.0",
    "illuminate/http": "^8.0|^9.0|^10.0|^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "scripts": {
    "pre-install-cmd": [
      "Arrowpay\\ArrowBaze\\Helpers\\License::validateComposer"
    ],
    "pre-update-cmd": [
      "Arrowpay\\ArrowBaze\\Helpers\\License::validateComposer"
    ]
  }
}
